{
	"size": 0,
	"aggregations": {
		"range": {
			"filter": {
				"range": {
					"start_date": {
						"from": "now-7d/d",
						"to": null,
						"include_lower": true,
						"include_upper": true,
						"boost": 1.0
					}
				}
			},
			"aggregations": {
				"dates": {
					"date_histogram": {
						"field": "start_date",
						"interval": "1d",
						"offset": 0,
						"order": {
							"_key": "asc"
						},
						"keyed": false,
						"min_doc_count": 0,
						"extended_bounds": {
							"min": "now-7d/d",
							"max": "now"
						}
					},
					"aggregations": {
						"power": {
							"avg": {
								"field": "Score"
							}
						},
						"attempts": {
							"filter": {
								"term": {
									"completed": {
										"value": true,
										"boost": 1.0
									}
								}
							},
							"aggregations": {
								"sessions": {
									"date_histogram": {
										"field": "start_date",
										"interval": "12h",
										"offset": 0,
										"order": {
											"_key": "asc"
										},
										"keyed": false,
										"min_doc_count": 0
									}
								}
							}
						},
						"missions": {
							"filter": {
								"terms": {
									"type": [
										"transference",
										"pvt"
									],
									"boost": 1.0
								}
							}
						}
					}
				}
			}
		},
		"playtime": {
			"sum": {
				"field": "duration"
			}
		},
		"users": {
			"cardinality": {
				"field": "user_id"
			}
		}
	}
}